        "doctype": "Workflow Transition"
      },
      {
        "state": "Warehouse Approved",
        "action": "Reject",
        "next_state": "Rejected",
        "allowed": "Kitting Supervisor",
        "doctype": "Workflow Transition"
      },
      {
        "state": "Rejected",
