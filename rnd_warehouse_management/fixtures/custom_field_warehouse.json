[
  {
    "doctype": "Custom Field",
    "dt": "Warehouse",
    "fieldname": "custom_requires_monitoring",
    "label": "Requires Environmental Monitoring",
    "fieldtype": "Check",
    "insert_after": "is_rejected_warehouse",
    "description": "Warehouse requires environmental parameter monitoring",
    "default": "0",
    "name": "Warehouse-custom_requires_monitoring"
  },
  {
    "doctype": "Custom Field",
    "dt": "Warehouse",
    "fieldname": "section_break_temperature",
    "label": "Temperature Monitoring",
    "fieldtype": "Section Break",
    "insert_after": "custom_requires_monitoring",
    "depends_on": "custom_requires_monitoring",
    "collapsible": 1,
    "collapsible_depends_on": "custom_requires_monitoring",
    "name": "Warehouse-section_break_temperature"
  },
  {
    "doctype": "Custom Field",
    "dt": "Warehouse",
    "fieldname": "custom_temperature_controlled",
    "label": "Temperature Controlled",
    "fieldtype": "Check",
    "insert_after": "section_break_temperature",
    "depends_on": "custom_requires_monitoring",
    "description": "Warehouse requires temperature monitoring",
    "default": "0",
    "name": "Warehouse-custom_temperature_controlled"
  },
  {
    "doctype": "Custom Field",
    "dt": "Warehouse",
    "fieldname": "custom_temperature_uom",
    "label": "Temperature Unit",
    "fieldtype": "Select",
    "insert_after": "custom_temperature_controlled",
    "depends_on": "custom_temperature_controlled",
    "options": "\u00b0C (Celsius)\n\u00b0F (Fahrenheit)\nK (Kelvin)",
    "default": "\u00b0C (Celsius)",
    "description": "Unit of measurement for temperature",
    "name": "Warehouse-custom_temperature_uom"
  },
  {
    "doctype": "Custom Field",
    "dt": "Warehouse",
    "fieldname": "custom_min_temperature",
    "label": "Minimum Temperature",
    "fieldtype": "Float",
    "insert_after": "custom_temperature_uom",
    "depends_on": "custom_temperature_controlled",
    "precision": 1,
    "description": "Lowest acceptable temperature",
    "name": "Warehouse-custom_min_temperature",
    "default": "0"
  },
  {
    "doctype": "Custom Field",
    "dt": "Warehouse",
    "fieldname": "custom_max_temperature",
    "label": "Maximum Temperature",
    "fieldtype": "Float",
    "insert_after": "custom_min_temperature",
    "depends_on": "custom_temperature_controlled",
    "precision": 1,
    "description": "Highest acceptable temperature",
    "name": "Warehouse-custom_max_temperature",
    "default": "0"
  },
  {
    "doctype": "Custom Field",
    "dt": "Warehouse",
    "fieldname": "custom_target_temperature",
    "label": "Target Temperature",
    "fieldtype": "Float",
    "insert_after": "custom_max_temperature",
    "depends_on": "custom_temperature_controlled",
    "precision": "9",
    "description": "Ideal/optimal temperature value",
    "name": "Warehouse-custom_target_temperature",
    "default": "0"
  },
  {
    "doctype": "Custom Field",
    "dt": "Warehouse",
    "fieldname": "custom_current_temperature",
    "label": "Current Temperature",
    "fieldtype": "Float",
    "insert_after": "custom_target_temperature",
    "depends_on": "custom_temperature_controlled",
    "precision": 1,
    "description": "Latest measured temperature",
    "name": "Warehouse-custom_current_temperature",
    "default": "0"
  },
  {
    "doctype": "Custom Field",
    "dt": "Warehouse",
    "fieldname": "column_break_temperature",
    "fieldtype": "Column Break",
    "insert_after": "custom_current_temperature",
    "name": "Warehouse-column_break_temperature"
  },
  {
    "doctype": "Custom Field",
    "dt": "Warehouse",
    "fieldname": "custom_temperature_spec_display",
    "label": "Temperature Specification",
    "fieldtype": "Data",
    "insert_after": "column_break_temperature",
    "depends_on": "custom_temperature_controlled",
    "read_only": 1,
    "bold": 1,
    "description": "Auto-generated: e.g., '2-8\u00b0C (Target: 5\u00b0C)'",
    "length": 100,
    "name": "Warehouse-custom_temperature_spec_display"
  },
  {
    "doctype": "Custom Field",
    "dt": "Warehouse",
    "fieldname": "custom_temperature_status",
    "label": "Temperature Status",
    "fieldtype": "Select",
    "insert_after": "custom_temperature_spec_display",
    "depends_on": "custom_temperature_controlled",
    "options": "Within Range\nBelow Minimum\nAbove Maximum\nNot Monitored",
    "default": "Not Monitored",
    "read_only": 1,
    "bold": 1,
    "description": "Automatically calculated based on current vs range",
    "name": "Warehouse-custom_temperature_status"
  },
  {
    "doctype": "Custom Field",
    "dt": "Warehouse",
    "fieldname": "custom_last_temperature_check",
    "label": "Last Temperature Check",
    "fieldtype": "Datetime",
    "insert_after": "custom_temperature_status",
    "depends_on": "custom_temperature_controlled",
    "read_only": 1,
    "description": "When temperature was last recorded",
    "name": "Warehouse-custom_last_temperature_check"
  },
  {
    "doctype": "Custom Field",
    "dt": "Warehouse",
    "fieldname": "section_break_humidity",
    "label": "Humidity Monitoring",
    "fieldtype": "Section Break",
    "insert_after": "custom_last_temperature_check",
    "depends_on": "custom_requires_monitoring",
    "collapsible": 1,
    "name": "Warehouse-section_break_humidity"
  },
  {
    "doctype": "Custom Field",
    "dt": "Warehouse",
    "fieldname": "custom_humidity_controlled",
    "label": "Humidity Controlled",
    "fieldtype": "Check",
    "insert_after": "section_break_humidity",
    "depends_on": "custom_requires_monitoring",
    "description": "Warehouse requires humidity monitoring",
    "default": "0",
    "name": "Warehouse-custom_humidity_controlled"
  },
  {
    "doctype": "Custom Field",
    "dt": "Warehouse",
    "fieldname": "custom_min_humidity",
    "label": "Minimum Humidity (%)",
    "fieldtype": "Percent",
    "insert_after": "custom_humidity_controlled",
    "depends_on": "custom_humidity_controlled",
    "description": "Lowest acceptable humidity level",
    "name": "Warehouse-custom_min_humidity",
    "precision": "9",
    "default": "0"
  },
  {
    "doctype": "Custom Field",
    "dt": "Warehouse",
    "fieldname": "custom_max_humidity",
    "label": "Maximum Humidity (%)",
    "fieldtype": "Percent",
    "insert_after": "custom_min_humidity",
    "depends_on": "custom_humidity_controlled",
    "description": "Highest acceptable humidity level",
    "name": "Warehouse-custom_max_humidity",
    "precision": "9",
    "default": "0"
  },
  {
    "doctype": "Custom Field",
    "dt": "Warehouse",
    "fieldname": "custom_target_humidity",
    "label": "Target Humidity (%)",
    "fieldtype": "Percent",
    "insert_after": "custom_max_humidity",
    "depends_on": "custom_humidity_controlled",
    "description": "Ideal humidity level",
    "name": "Warehouse-custom_target_humidity",
    "precision": "9",
    "default": "0"
  },
  {
    "doctype": "Custom Field",
    "dt": "Warehouse",
    "fieldname": "custom_current_humidity",
    "label": "Current Humidity (%)",
    "fieldtype": "Percent",
    "insert_after": "custom_target_humidity",
    "depends_on": "custom_humidity_controlled",
    "description": "Latest measured humidity",
    "name": "Warehouse-custom_current_humidity",
    "precision": "9",
    "default": "0"
  },
  {
    "doctype": "Custom Field",
    "dt": "Warehouse",
    "fieldname": "custom_warehouse_code",
    "label": "Warehouse Code",
    "fieldtype": "Data",
    "insert_after": "custom_current_humidity",
    "description": "Warehouse identification code",
    "name": "Warehouse-custom_warehouse_code"
  },
  {
    "doctype": "Custom Field",
    "dt": "Warehouse",
    "fieldname": "section_break_zone_config",
    "label": "Zone Configuration",
    "fieldtype": "Section Break",
    "insert_after": "custom_warehouse_code",
    "collapsible": 1,
    "name": "Warehouse-section_break_zone_config"
  },
  {
    "doctype": "Custom Field",
    "dt": "Warehouse",
    "fieldname": "custom_is_zone_warehouse",
    "label": "Is Zone Warehouse",
    "fieldtype": "Check",
    "insert_after": "section_break_zone_config",
    "description": "Check if this warehouse represents a Red/Green zone",
    "name": "Warehouse-custom_is_zone_warehouse"
  },
  {
    "doctype": "Custom Field",
    "dt": "Warehouse",
    "fieldname": "custom_zone_type",
    "label": "Zone Type",
    "fieldtype": "Select",
    "options": "\nRed Zone\nGreen Zone",
    "insert_after": "custom_is_zone_warehouse",
    "depends_on": "custom_is_zone_warehouse",
    "description": "Type of zone this warehouse represents",
    "name": "Warehouse-custom_zone_type"
  },
  {
    "doctype": "Custom Field",
    "dt": "Warehouse",
    "fieldname": "custom_max_capacity",
