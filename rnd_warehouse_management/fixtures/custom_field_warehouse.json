    "precision": 2,
    "description": "Current warehouse utilization percentage",
    "name": "Warehouse-custom_current_utilization",
    "default": "0"
  },
  {
    "doctype": "Custom Field",
    "dt": "Warehouse",
    "fieldname": "custom_last_capacity_update",
    "label": "Last Capacity Update",
    "fieldtype": "Datetime",
    "insert_after": "custom_current_utilization",
    "read_only": 1,
    "name": "Warehouse-custom_last_capacity_update"
  }
