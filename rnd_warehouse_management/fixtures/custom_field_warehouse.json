[
 {
  "doctype": "Custom Field",
  "dt": "Warehouse",
  "fieldname": "custom_temperature_control",
  "label": "Temperature Control",
  "fieldtype": "Check",
  "insert_after": "is_rejected_warehouse",
  "description": "Enable if this warehouse requires temperature monitoring",
  "name": "Warehouse-custom_temperature_control"
 },
 {
  "doctype": "Custom Field",
  "dt": "Warehouse",
  "fieldname": "custom_target_temperature",
  "label": "Target Temperature (\u00b0C)",
  "fieldtype": "Float",
  "insert_after": "custom_temperature_control",
  "depends_on": "custom_temperature_control",
  "precision": 1,
  "description": "Target temperature in Celsius for this warehouse",
  "name": "Warehouse-custom_target_temperature"
 },
 {
  "doctype": "Custom Field",
  "dt": "Warehouse",
  "fieldname": "custom_warehouse_code",
  "label": "Warehouse Code",
  "fieldtype": "Data",
  "insert_after": "custom_target_temperature",
  "description": "Warehouse identification code",
  "name": "Warehouse-custom_warehouse_code"
 },
 {
  "doctype": "Custom Field",
  "dt": "Warehouse",
  "fieldname": "section_break_zone_config",
  "label": "Zone Configuration",
  "fieldtype": "Section Break",
  "insert_after": "custom_warehouse_code",
  "collapsible": 1,
  "name": "Warehouse-section_break_zone_config"
 },
 {
  "doctype": "Custom Field",
  "dt": "Warehouse",
  "fieldname": "custom_is_zone_warehouse",
  "label": "Is Zone Warehouse",
  "fieldtype": "Check",
  "insert_after": "section_break_zone_config",
  "description": "Check if this warehouse represents a Red/Green zone",
  "name": "Warehouse-custom_is_zone_warehouse"
 },
 {
  "doctype": "Custom Field",
  "dt": "Warehouse",
  "fieldname": "custom_zone_type",
  "label": "Zone Type",
  "fieldtype": "Select",
  "options": "\nRed Zone\nGreen Zone",
  "insert_after": "custom_is_zone_warehouse",
  "depends_on": "custom_is_zone_warehouse",
  "description": "Type of zone this warehouse represents",
  "name": "Warehouse-custom_zone_type"
 },
 {
  "doctype": "Custom Field",
  "dt": "Warehouse",
  "fieldname": "custom_max_capacity",
  "label": "Maximum Capacity",
  "fieldtype": "Float",
  "insert_after": "custom_zone_type",
  "description": "Maximum storage capacity for this warehouse",
  "name": "Warehouse-custom_max_capacity"
 },
 {
  "doctype": "Custom Field",
  "dt": "Warehouse",
  "fieldname": "column_break_capacity",
  "fieldtype": "Column Break",
  "insert_after": "custom_max_capacity",
  "name": "Warehouse-column_break_capacity"
 },
 {
  "doctype": "Custom Field",
  "dt": "Warehouse",
  "fieldname": "custom_current_utilization",
  "label": "Current Utilization %",
  "fieldtype": "Percent",
  "insert_after": "column_break_capacity",
  "read_only": 1,
  "precision": 2,
  "description": "Current warehouse utilization percentage",
  "name": "Warehouse-custom_current_utilization"
 },
 {
  "doctype": "Custom Field",
  "dt": "Warehouse",
  "fieldname": "custom_last_capacity_update",
  "label": "Last Capacity Update",
  "fieldtype": "Datetime",
  "insert_after": "custom_current_utilization",
  "read_only": 1,
  "name": "Warehouse-custom_last_capacity_update"
 }
]
